<%= render @user %>

<div>
  <div class="">
    <%=
      button_to "Edit Profile",
      edit_profile_path(@user),
      method: :get,
      class: "btn btn-secondary"
    %>
  </div>
  <div class="">
    <%=
      button_to "Delete Profile",
      profile_path(@user),
      method: :delete,
      class: "btn btn-danger"
    %>
  </div>
</div>
